@model IEnumerable<TheTjerrs.Models.Partite>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.EmriPartise)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Epoll)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.EmriPartise)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Epoll.EpollId)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.PartiId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.PartiId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.PartiId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
